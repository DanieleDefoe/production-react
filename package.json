    "loki": "^0.35.1",
    "mini-css-extract-plugin": "^2.9.1",
    "prettier": "^3.3.3",
    "react-docgen-typescript-plugin": "^1.0.8",
    "react-refresh": "^0.14.2",
    "react-refresh-typescript": "^2.0.9",
    "reg-cli": "^0.18.8",
